{
	"name": "MXCK2 Workspace",
	"dockerComposeFile": "docker-compose.yml",
	"service": "development",
	"workspaceFolder": "/mxck2_ws",
	"customizations": {
		"vscode": {
		"extensions": [
			"ms-python.python",
			"ms-azuretools.vscode-docker"
		],
		"settings": {
			"python.defaultInterpreterPath": "/usr/bin/python3",
			"python.analysis.extraPaths": ["/mxck2_ws/src"]
		}
		}
	}
}
